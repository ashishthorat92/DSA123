#include <stdio.h>
#define MAX 5 
int queue[MAX];
int front = -1, rear = -1;
void addParcel(int parcel) 
{
if (rear == MAX - 1) 
{
printf("Queue Overflow! Cannot add parcel %d\n", parcel);
} 
else 
{
if (front == -1) 
front = 0; rear++;
queue[rear] = parcel;
printf("Parcel %d added successfully.\n", parcel);
}
}
void removeParcel() 
{
if 
(front == -1 || front > rear) 
{
printf("Queue Underflow! No parcels to remove.\n");
} 
else 
{
printf("Parcel %d dispatched.\n", queue[front]); 
front++;
}
}
void displayParcels()
{
if (front == -1 || front > rear) 
{ 
printf("No parcels in queue.\n");
} 
else 
{
printf("Parcels in queue: ");
for (int i = front; i <= rear; i++) 
{ 
printf("%d ", queue[i]);
}
printf("\n");
}
}
int main() 
{
int choice, parcel;
do 
{
printf("\n--- Post Office Parcel Handling ---\n"); 
printf("1. Add Parcel\n");
printf("2. remove Parcel\n"); 
printf("3. Display Parcels\n");
printf("4. Exit\n"); 
printf("Enter your choice: "); 
scanf("%d", &choice); 
switch(choice) 
{
case 1:
printf("Enter parcel number: "); 
scanf("%d", &parcel); 
addParcel(parcel);
break; 
case 2:
removeParcel(); 
break;
case 3:
displayParcels(); 
break;
case 4:
printf("Exiting...\n"); 
break;
default:
printf("Invalid choice!\n");
} 
}
while(choice != 4); 
return 0;
}
